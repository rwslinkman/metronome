{
    "name": "rwslinkman/metronome",
    "description": "Test utility for Symfony 3 (PHP) applications",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Rick Slinkman",
            "email": "rw.slinkman@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
          "Metronome\\": "src/Metronome/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Metronome\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.3.0",
        "doctrine/orm": "^2.5",
        "symfony/finder": "~4.0|~3.0",
        "mockery/mockery": "^1.2",
        "doctrine/doctrine-fixtures-bundle": "~3.0|~2.3",
        "symfony/dom-crawler": "^4.1",
        "symfony/browser-kit": "^4.3",
        "symfony/security-bundle": "^4.1"
    },
    "require-dev": {
        "pdepend/pdepend": "^2.5",
        "sebastian/phpcpd": "^4.1",
        "squizlabs/php_codesniffer": "^3.2",
        "phploc/phploc": "^4.0",
        "phpmd/phpmd": "^2.6",
        "phpunit/phpunit": "^7.4",
        "symfony/yaml": "^4.1"
    }
}
