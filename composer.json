{
    "name": "rwslinkman/metronome",
    "description": "Test utility for Symfony 3 (PHP) applications",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Rick Slinkman",
            "email": "rw.slinkman@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
          "Metronome\\": "src/Metronome/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Metronome\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.1",
        "symfony/dom-crawler": "^4.1|^5.0|^6.1",
        "symfony/yaml": "^6.1",
        "doctrine/orm": "^2.12",
        "symfony/form": "^6.1",
        "symfony/browser-kit": "^6.1",
        "symfony/security-bundle": "^6.1",
        "symfony/finder": "^6.1",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "mockery/mockery": "^1.5",
        "ext-json": "*"
    },
    "require-dev": {
        "pdepend/pdepend": "^2.5",
        "sebastian/phpcpd": "^6.0",
        "squizlabs/php_codesniffer": "^3.2",
        "phploc/phploc": "^7.0",
        "phpmd/phpmd": "^2.6",
        "phpunit/phpunit": "^9.5"
    }
}
