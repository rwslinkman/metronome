{
    "name": "rwslinkman/metronome",
    "description": "Test utility for Symfony 3 (PHP) applications",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Rick Slinkman",
            "email": "rw.slinkman@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
          "Metronome\\": "src/Metronome/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Metronome\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.3",
        "doctrine/orm": "^2.7",
        "symfony/finder": "~4.4|~3.0",
        "mockery/mockery": "^1.2",
        "doctrine/doctrine-fixtures-bundle": "~3.3|~2.3",
        "symfony/dom-crawler": "^4.1|^5.0",
        "symfony/browser-kit": "^4.3|^5.0",
        "symfony/security-bundle": "^4.1|^5.0",
        "symfony/form": "^4.1|^5.0",
        "ext-json": "*",
        "ramunasd/symfony-container-mocks": "^0.6.0"
    },
    "require-dev": {
        "pdepend/pdepend": "^2.5",
        "sebastian/phpcpd": "^4.1",
        "squizlabs/php_codesniffer": "^3.2",
        "phploc/phploc": "^4.0",
        "phpmd/phpmd": "^2.6",
        "phpunit/phpunit": "^7.4",
        "symfony/yaml": "^4.1"
    }
}
